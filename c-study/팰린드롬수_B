#include<stdio.h>
int main()
{
	unsigned int n;
	unsigned int str_1[101];
	unsigned int str_2[101];
	scanf("%d", &n);
	int cnt=0;
	int index = 0;
	int result = 0;
	int sum = 0,sum_2=0;
	
	while (1)
	{
		str_1[cnt] = n % 10;
		n = n / 10;
		if (n == 0)
			break;
		cnt++;
	}
	for(int i=cnt;i>=0;i--)
	{
		str_2[index] = str_1[i];
		index++;   
	}

	for (int i = 0; i < cnt; i++)
	{
		if (str_1[i] == str_2[i])
		{
			result++;
		}
	}
	

	for (int i = 0; i <=cnt; i++) {
		sum = sum * 10 + str_1[i];

	}

	if (result == cnt)
	{
		printf("palindrome");
	}
	else 
	{
		printf("%d", sum);
		
	}
	return 0;
}

#include <stdio.h>
 
int main() {
    int n;
    scanf("%d", &n);
    getchar(); 
 
    while (n--)
    {
        char input[9]; 
        scanf("%8s", input);
 
        unsigned char result = 0;
 
        for (int i = 0; i < 8; i++) 
        {
            if (input[i] == '-')
            {
                result |= (1 << (7 - i));
            }
        }
 
        printf("%d ", result);
    }
 
    printf("\n");
 
    return 0;
}

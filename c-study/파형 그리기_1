#include <stdio.h>
 
int main() {
    int n;
    scanf("%d", &n);
 
    for (int i = 0; i < n; i++) {
        int value;
        scanf("%d", &value);
 
        for (int j = 7; j >= 0; j--) 
{
            if (value & (1 << j)) 
{
                printf("-");
            }
            else {
                printf("_");
            }
        }
    }
 
    printf("\n");
 
    return 0;
}

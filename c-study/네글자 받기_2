#include <stdio.h>
 
int main() {
    unsigned int num;
 
    scanf("%u", &num);
 
    char str[5];
 
  
    for (int i = 0; i < 4; i++) 
    {
        str[i] = ((num >> (i * 8)) & 0xFF); 
    }
    str[4] = NULL; 
 
    for (int i = 0; i <4; i++)
    {
        printf("%c", str[i]);
    }
    return 0;
}

#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
int isPrime(int n)
{
    int cnt = 0;
    int i = 0;
 
    for (i = 2; i < n; i++) 
    {
        if (n % i == 0)
            break;
    }
    if (i == n)
    {
        return 0;
    }
    else
    {
        return 1;
    }
}

int main(int argc, char const* argv[])
{
    int a, b;
    scanf("%d%d", &a, &b);

    int cnt = 0;


    for (int n = a; n <= b; n++)
    {
        cnt += isPrime(n);
    }

    printf("%d", cnt);

    return 0;
}

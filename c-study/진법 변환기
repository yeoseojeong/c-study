#include <stdio.h>
 
int main() {
    int n, x;
  
    scanf("%d %d", &n, &x);
 
    if (x < 2 || x >= 10) 
    {
       
        return 1; 
    }
 
    if (n == 0)
    {
        printf("0\n");
        return 0; 
    }
 
    char result[32];  
    int index = 0;
 
    while (n > 0) 
    {
        int mod =  n% x;
        result[index++] = '0' + mod;  
        n /= x;
    }
 
   
    for (int i = index - 1; i >= 0; i--) 
    {
        printf("%c", result[i]);
    }
 
    printf("\n");
 
    return 0; 
}

#include<stdio.h>
int main()
{
	unsigned int n;
	unsigned int arr[50];
	int cnt=1;

	scanf("%d", &n);
	while (1)
	{
		arr[cnt] = n % 10;
		n = n / 10;
	
		if (n == 0)
		{
			break;
		}
		cnt++;	
	}
	for (int i = cnt; i >=1 ; i--)
	{
		if (i % 2 == 0)
		{
			printf("%d", arr[i]);
		}
	}
	return 0;
}

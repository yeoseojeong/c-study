//대소문자 바꾸기


#include<stdio.h>
int main()
{
    char str[31];
 
    scanf("%s", &str);
 
    for (int i = 0; str[i] != NULL; i++)
    {
        if (str[i] >= 'a' && str[i] <= 'z')
        {
            str[i] = str[i];
        }
        else if (str[i] >= 'A' && str[i] <= 'Z')
        {
            str[i] = str[i] + 32;
        }
         
    }
    printf("%s", str);
    return 0;
}


#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>

int main()
{
    int str[11];
    int max = -100000;
    int index;

    for (int i = 0; i < 10; i++)
    {
        scanf("%d", &str[i]);
    }

    for(int i=0;i<10;i++)
    {
        if (str[i] > max)
        {
            max = str[i];
        }
    }

    for (int i = 0; i < 10; i++)
    {
        if (str[i] == max)
        {
            index = i;
        }
    }
    printf("%d %d\n", max, index);

    return 0;

}

//int 변수의 첫 바이트와 네번 째 바이트의 위치를 바꿔 저장하고 두번 째 바이트와 세번 째 바이트의 위치를 바꿔 저장


#include <stdio.h>
 
int main() {
    unsigned int n;
    unsigned int result = 0;
 
        scanf("%u", &n);
    
 
    result |= (n & 0xFF) << 24;
    result |= ((n >> 8) & 0xFF) << 16;
    result |= ((n >> 16) & 0xFF) << 8;
    result |= ((n >> 24) & 0xFF);
 
    
   
        printf("%u\n", result);
   
 
    return 0;
}

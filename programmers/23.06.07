//원소들의 곱과 합

#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

int solution(int num_list[], size_t num_list_len) {
    int answer = 0;

    int sum = 0;
    int multi = 1;

    for (int i = 0; i< num_list_len; i++)
    {
        multi *= num_list[i];
    }
    for (int i = 0; i < num_list_len; i++)
    {
        sum += num_list[i];
    }

    sum = sum * sum;

    if (multi > sum)
    {
        answer = 0;
    }
    else
    {
        answer = 1;
    }

    return answer;
}

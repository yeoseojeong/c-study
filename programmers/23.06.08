//글자 이어붙여 문자열 만들기


#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>


char* solution(const char* string, int index_list[], size_t index_list_len) {
    
    int idx = 0;

    char* answer = (char*)malloc(index_list);


    for (int i = 0; i < index_list_len; i++)
    {
        idx=index_list[i];
        answer[i]=string[idx];
    }
  


    return answer;
}

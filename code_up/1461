//이차원 배열 배우기  1461

#include <stdio.h>
int main()
{
	int n,m=1;
	int arr[100][100] = { 0 };
	scanf("%d", &n);
	for (int i = 1; i <= n; i++)
	{
		
		for (int j =n; j >= 1; j--)
		{
			arr[i][j] = m;
			m++;
		
		}
		
	}

	for (int i = 1; i <= n; i++)
	{
		for (int j = 1; j <= n; j++)
		{
			printf("%d ", arr[i][j]);
		}
		printf("\n");
	}

	
}


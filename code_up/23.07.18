//비밀 전화번호

#include<stdio.h>
int main()
{

	char str[20];
	char data[20];

	gets_s(str);
	gets_s(data);
	

	for (int i = 0; data[i] != NULL; i++)
	{
		for (int j = 0; str[j] != NULL; j++)
		{
			if (data[i] == str[j])
			{
				printf("%d", j);
				break;
			}
			else if(data[i]==' ')
			{
				printf(" ");
				break;
			}
		}
	}
	return 0;

}


//이차원배열 배우기 1460

#include <stdio.h>
int main()
{
	int n,m=1;
	int arr[100][100] = { 0 };
	scanf("%d", &n);
	for (int i = 1; i <= n; i++)
	{
		
		for (int j =1; j <= n; j++)
		{
			arr[i][j] = m;
			m++;
			printf("%d ", arr[i][j]);
		
		}
		printf("\n");
	}

	
}


//이차원 배열 배우기 -2 1461

#include <stdio.h>
int main()
{
	int n,m=1;
	int arr[100][100] = { 0 };
	scanf("%d", &n);
	for (int i = 1; i <= n; i++)
	{
		
		for (int j =n; j >= 1; j--)
		{
			arr[i][j] = m;
			m++;
		
		}
		
	}

	for (int i = 1; i <= n; i++)
	{
		for (int j = 1; j <= n; j++)
		{
			printf("%d ", arr[i][j]);
		}
		printf("\n");
	}

	
}


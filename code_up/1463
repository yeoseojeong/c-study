//이차원 배열 배우가 1463

#include <stdio.h>
int main()
{
	int n,m=1;
	int arr[101][101] = {  };
	scanf("%d", &n);
	for (int i = 1; i <= n; i++)
	{
		
		for (int j =n; j >= 1; j--)
		{
			arr[j][i] = m;
			m++;
		
		}
		
	}

	for (int i = 1; i <= n; i++)
	{
		for (int j = 1; j <= n; j++)
		{

			printf("%d ", arr[i][j]);
		}
		printf("\n");
	}

	
}

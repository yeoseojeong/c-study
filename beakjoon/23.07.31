#include <stdio.h>
#include <string.h>

int main(void)
{
    int n, i, j, sum, add;             
    char arr[81];

    scanf("%d", &n);

    for (i = 0; i < n; i++)
    {
        sum = 0, add = 1;
        scanf("%s", arr);
        for (j = 0; j < strlen(arr); j++)   
        {
            if (arr[j] == 'O')
            {
                sum += add;
                add++;
            }
            else
                add = 1;
        }
        printf("%d\n", sum);
    }
    return 0;
}


//2920

#include<stdio.h>
int main()
{
	int melody[8] = { 0 };
	int cnt = 0;

	for (int i = 0; i < 8; i++)
	{
		scanf("%d", &melody[i]);
	}
	if (melody[0] == 1)
	{
		for (int i = 1; i < 8; i++)
		{
			if (melody[i] == i + 1)
			{
				cnt = 1;
			}
			else
			{
				cnt = 0;
				break;
			}
		}
	}
	else if (melody[0] == 8)
	{
		for (int i = 1; i < 8; i++)
		{
			if (melody[i] == 8 - i)
			{
				cnt = 2;
			}
			else
			{
				cnt = 0;
				break;
			}
		}
	}


	if (cnt == 1)
	{
		printf("ascending");
	}
	else if (cnt == 2)
	{
		printf("descending");
	}
	else
	{
		printf("mixed");
	}

	return 0;
}


//1977

#include <stdio.h>

int main() {
	int n, m, i, sum = 0, min = 99999;
	scanf("%d %d", &n, &m);

	for (i = 1; i <= 100; i++) {
		
		if (n <= i * i && i * i <= m) {
			sum += i * i;
			if (min > i * i)
				min = i * i;
		}
	}
	if (sum == 0) printf("-1");
	else printf("%d\n%d", sum, min);
}

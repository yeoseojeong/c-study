//1009

#include<stdio.h>
int main()
{
	int t;
	int a, b;
	int result=1;
	
	scanf("%d", &t);

	for (int i = 0; i < t; i++)
	{
		scanf("%d %d", &a, &b);

		for (int j = 0; j < b; j++)
		{
			result *= a;
			result %= 10;
		}
	
		if (result == 0)
		{
			result = 10;
		}
		printf("%d\n", result);
		result = 1;


	}
	return 0;
}
		


	
